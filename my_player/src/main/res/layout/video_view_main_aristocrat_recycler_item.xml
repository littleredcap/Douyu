<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_marginBottom="10dp">

    <ImageView
        android:id="@+id/video_view_main_aristocrat_item_rank_name"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_gravity="center"
        android:src="@android:drawable/btn_star_big_on" />

    <liang.zhou.lane8.no5.my_player.ui.MyCircleImageView
        android:id="@+id/video_view_main_aristocrat_item_portrait"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:src="@mipmap/yukee"/>

    <TextView
        android:id="@+id/video_view_main_aristocrat_item_fansName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="陈钰琪_yukee"
        android:textColor="@android:color/black"
        android:layout_gravity="center"/>

</LinearLayout>